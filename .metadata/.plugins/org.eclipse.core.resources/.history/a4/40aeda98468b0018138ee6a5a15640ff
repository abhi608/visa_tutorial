package com.visa.prj.service;

import java.util.List;

public class DataService {
	
	private List<String> list;

	public void setList(List<String> list) {
		this.list = list;
	}
	
	public int getSize() {
		return list.size();
	}
	
	public void add(List<String> s1) {
		for(String s : s1) {
			list.add(s);
		}
	}
	
	public void remove(String s) {
		list.remove(s);
	}

}
