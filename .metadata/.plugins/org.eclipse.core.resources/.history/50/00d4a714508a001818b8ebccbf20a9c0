package com.visa.prj.client;

import java.lang.reflect.InvocationTargetException;
import java.lang.reflect.Method;

import com.visa.prj.entity.Product;

public class ProductClient {
	public static void main(String[] args) {
		Product[] products = new Product[4];
		products[0] = new Product(124, "iPhone x", 120000.00, "mobile"); // upcasting
		products[1] = new Product(634, "Onida", 3500.00, "tv");
		products[2] = new Product(611, "MotoG", 12999.00, "mobile");
		products[3] = new Product(72, "Sony Bravia", 103500.00, "computer");
		
		
	}
	
}