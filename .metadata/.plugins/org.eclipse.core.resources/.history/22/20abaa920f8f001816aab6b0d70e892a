package com.visa.prj.entity;

public class Tv extends Item {
	private String screen;
	
	public Tv() {
	}

	public Tv(int id, String name, double price, String screen) {
		super(id, name, price);
		this.screen = screen;
	}
	
	public String getScreen() {
		return screen;
	}

	public void setScreen(String screen) {
		this.screen = screen;
	}
	
	
}
